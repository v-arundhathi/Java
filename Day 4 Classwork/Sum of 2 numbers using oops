import java.io.*;
import java.util.*;
class Add2
{
	void add_2_no(){
	Scanner s=new Scanner(System.in);
	System.out.println("Enter first number: ");
	int a=s.nextInt();
	System.out.println("Enter second number: ");
	int b=s.nextInt();
	System.out.println("Sum of numbers: "+(a+b));
	}
	}
	class Sumof2
	{
		public static void main(String args[])
		{
			Add2 A= new Add2();
			A. add_2_no();
		}
	}
	
