import java.io.*;
import java.util.*;
class Arraysum
{
public static void main(String args[])
{
Scanner s= new Scanner(System.in);
int n=s.nextInt();
n=5;

int a[]=new int[n];
int i;
System.out.println("Enter elements of the array:");
for(i=0;i<n;i++){
a[i]=s.nextInt();
}


System.out.println("sum of the array:"+(a[0]+a[n-1]));
System.out.println("Difference of the array:"+(a[0]-a[n-1]));


}
}
