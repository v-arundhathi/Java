import java.io.*;
import java.util.*;
class Arrayseclar
{
public static void main(String args[])
{
Scanner s= new Scanner(System.in);
int a[]=new int[5];
int i,j,temp;
System.out.println("Enter elements of the array:");

for(i=0;i<5;i++){
a[i]=s.nextInt();
}

for(i=0;i<5;i++){
for(j=i+1;j<5;j++)
{
if(a[i]<a[j]){
temp=a[i];
a[i]=a[j];
a[j]=temp;
}
}
}
System.out.print("Second laregest element:"+a[1]);
}
}
