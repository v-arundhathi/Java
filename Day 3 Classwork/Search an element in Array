import java.io.*;
import java.util.*;
class Arraysearch
{
public static void main(String args[])
{
Scanner s= new Scanner(System.in);
int a[]=new int[5];

int i,c=0;
System.out.println("Enter elements of the array:");

for(i=0;i<5;i++){
a[i]=s.nextInt();
}
System.out.println("Enter the vakue to be searched in the array:");
int v=s.nextInt();

for(i=0;i<5;i++){
if(v==a[i])
	System.out.println("index position "+(i+1));
	c++;

}
if(c==0)
	System.out.println("Not found");


}
}
