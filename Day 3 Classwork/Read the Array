import java.io.*;
import java.util.*;
class ArrayRead
{
public static void main(String args[])
{
Scanner s= new Scanner(System.in);
int a[]=new int[5];
int i;
System.out.println("Enter elements of the array:");

for(i=0;i<5;i++){
a[i]=s.nextInt();
}
System.out.println(" Elements of the array:");

for(i=0;i<5;i++){
System.out.println(a[i]);
}
}
}
