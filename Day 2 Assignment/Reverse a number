import java.io.*;
import java.util.*;
class Main
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
System.out.println("Enter value of n=");
int n=s.nextInt();
int rem,sum=0;
do{
    rem=n%10;
    sum=sum*10+rem;
    n=n/10;
}while(n!=0);
System.out.println("Reversed number:"+sum);
}}
