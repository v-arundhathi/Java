import java.io.*;
import java.util.*;
class Leap
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int ny;

System.out.println("Enter year:");
int year=s.nextInt();
ny=year%4+year;

if(year%4==0)
System.out.println("is leap year");
else
System.out.println("The next leap is year is "+(ny));
}
}
