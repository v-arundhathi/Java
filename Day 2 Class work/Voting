import java.io.*;
import java.util.*;
class Voting
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
System.out.println("Enter age:");
int age=s.nextInt();
if (age>=18)
System.out.println("Eligible for voting");
else
System.out.println("Not Eligible for voting still "+(18-age)+" years is remaining.");
}}
